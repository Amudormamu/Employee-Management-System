CREATE Database: `employee_dp`;
CREATE TABLE `users` (
  `id` int(10) UNSIGNED NOT NULL,
  `name` varchar(255) NOT NULL,
  `surname` varchar(255) NOT NULL,
  `email` varchar(255) NOT NULL,
  `position` varchar(255) NOT NULL,
  `date` date NOT NULL,
  `department` enum('Administrative','Human Resources','Sales','Marketing','Accounting','I.T') NOT NULL
);
